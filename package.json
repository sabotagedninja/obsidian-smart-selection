    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^8.57.1",
    "eslint-plugin-obsidianmd": "^0.1.8",
    "eslint-utils": "^3.0.0",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.4",
    "tslib": "^2.4.0",
